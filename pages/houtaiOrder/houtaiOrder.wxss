/* pages/houtaiShop/houtaiShop.wxss */
.Row{
    width: 95%;
    margin: 20rpx auto;
    display: flex;
    flex-direction: row;
    padding-top: 20rpx;
}
.Row>input{
    margin-left: 20rpx;
    width: 70%;
}
.shopDetailTips>textarea{
    border: solid 2rpx #D3D3D3;
    border-radius: 20rpx;
    width: 75%;
    margin-left: 20rpx;
    height: 200rpx;
}
.photosOne{
    width: 95%;
    margin:  auto;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    align-self: start;
    
}
.photosOne_Row{
    width: 30%;
    height: 200rpx;
    margin-left: 15rpx;
    padding-top: 20rpx;
}
.photosOne_Row_ImgOne{
    position: relative;
    top: 30rpx;
    right: 10rpx;
}

.shuxingGroupsButton{
    width: 95%;
    height: 80rpx;
    background-color: cornflowerblue;
    border-radius: 20rpx;
    margin: 10rpx auto;
    text-align: center;
    color: white;
    line-height: 2;
    font-size: 37rpx;
    font-weight: 550;
}

.shuxingRow{
    width: 95%;
    margin: 20rpx auto;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    
}
.shuxingRowLeft{
    width: 30%;
}
.shuxingRowLeft>view{
    width: 90%;
    height: 70rpx;
    border-radius: 20rpx;
    background-color: cornflowerblue;
    color: white;
    font-weight: 550;
    line-height: 2;
    margin-top: 15rpx;
    text-align: center;
}


.shuxingRowRight{
    width: 70%;
}
